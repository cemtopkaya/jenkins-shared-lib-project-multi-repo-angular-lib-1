{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@kapsam1/kutup11": {
      "projectType": "library",
      "root": "projects/kapsam1/kutup11",
      "sourceRoot": "projects/kapsam1/kutup11/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kapsam1/kutup11/tsconfig.lib.json",
            "project": "projects/kapsam1/kutup11/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kapsam1/kutup11/src/test.ts",
            "tsConfig": "projects/kapsam1/kutup11/tsconfig.spec.json",
            "karmaConfig": "projects/kapsam1/kutup11/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kapsam1/kutup11/tsconfig.lib.json",
              "projects/kapsam1/kutup11/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@kapsam1/kutup12": {
      "projectType": "library",
      "root": "projects/kapsam1/kutup12",
      "sourceRoot": "projects/kapsam1/kutup12/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kapsam1/kutup12/tsconfig.lib.json",
            "project": "projects/kapsam1/kutup12/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kapsam1/kutup12/src/test.ts",
            "tsConfig": "projects/kapsam1/kutup12/tsconfig.spec.json",
            "karmaConfig": "projects/kapsam1/kutup12/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kapsam1/kutup12/tsconfig.lib.json",
              "projects/kapsam1/kutup12/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "@kapsam1/kutup11"
}